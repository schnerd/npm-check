{
  "name": "npm-satisfy",
  "version": "1.0.0",
  "description": "Check for outdated, incorrect, and unused dependencies.",
  "main": "lib",
  "scripts": {
    "test": "xo && grunt test && bin/cli.js"
  },
  "xo": {
    "space": 4,
    "rules": {
      "no-warning-comments": [
        0
      ],
      "global-require": [
        0
      ]
    }
  },
  "bin": {
    "npm-satisfy": "bin/cli.js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/schnerd/npm-satisfy.git"
  },
  "keywords": [
    "npm",
    "outdated",
    "dependencies",
    "unused",
    "changelog",
    "check",
    "updates",
    "api",
    "interactive",
    "cli",
    "safe",
    "updating",
    "updater",
    "installer",
    "devDependencies"
  ],
  "author": {
    "name": "David Schnurr"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/schnerd/npm-satisfy/issues"
  },
  "homepage": "https://github.com/schnerd/npm-satisfy",
  "dependencies": {
    "chalk": "^1.1.0",
    "commander": "^2.9.0",
    "globby": "^3.0.1",
    "lodash": "^3.10.0",
    "node-emoji": "^1.0.3",
    "package-json": "^2.0.1",
    "q": "^1.4.1",
    "semver": "^5.0.1",
    "semver-diff": "^2.0.0",
    "text-table": "^0.2.0"
  },
  "devDependencies": {
    "grunt": "^0.4.0",
    "grunt-cli": "^0.1.13",
    "grunt-mocha-test": "^0.12.7",
    "grunt-release": "^0.13.0",
    "load-grunt-tasks": "^3.3.0",
    "mocha": "^2.3.3",
    "time-grunt": "^1.2.1",
    "xo": "^0.11.2"
  }
}
